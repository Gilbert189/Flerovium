{
	"attrs_version":1,
	"inc_commands":["estimate","threadInfo"],
	"replace":
	{
		"coin": 
		{
			"^(.+)$": ":coin: \\1"
		},
		"dice": 
		{
			"^(.+)$": ":game_die: \\1",
			"(You can't)": ":no_entry_sign: \\1",
			"(You roll no dice)": ":wind_chime: \\1",
			"(You roll \\d+ pieces of air)": ":wind_chime: \\1",
			"(?:(:.+?:) *)+":"\\1 "
		},
		"rollADice": 
		{
			"^(You rolled a )": ":game_die: \\1",
			"^(You can't)": ":no_entry_sign: \\1"
		},
		"suggest": 
		{
			"^(.+)$": ":pencil: \\1"
		},
		"files": 
		{
			"^(.+)$": ":file_folder: \\1"
		},
		"text": 
		{
			"^(.+)$": ":pencil: \\1"
		}
	}
}
